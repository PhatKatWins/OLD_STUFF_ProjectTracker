<Page x:Class="ProjectTracker.View.Pages.AdministratorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProjectTracker.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdministratorPage">

    <Grid>
        <TabControl Margin="10 0 10 10">
            <TabItem Header="Projects" Width="100" Height="20">
                <Frame Source="..\Pages\AdminPanelPages\ProjectsPage.xaml" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Users" Width="100" Height="20"/>
            <TabItem Header="Settings" Width="100" Height="20"/>
        </TabControl>
    </Grid>
</Page>
